<view class="cateList"><block wx:if="{{cateObj.category}}"><swiper class="swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="true"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{item}}" mode></image></view></swiper-item></block></swiper></block><view class="title">{{''+cateObj.category.frontName+''}}</view><view class="desc">{{''+cateObj.category.frontDesc+''}}</view><view class="goodsList"><block wx:for="{{cateObj.itemList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['cateObj.itemList','id',item.id]]]]]]]}}" class="goodsItem" bindtap="__e"><image class="goodsImg" src="{{item.primaryPicUrl}}" mode></image><view class="goodsDesc">{{''+item.name+''}}</view><view class="goodsPrice">{{''+item.retailPrice+''}}</view></view></block></view></view>